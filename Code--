rd_expenditure_2024 = year_2024.groupby('City')['R&D Expenditure (% of GDP)'].sum().reset_index()
print('R&D Expenditure (% of GDP) (2024):\n',rd_expenditure_2024)

# Plot

plt.figure(figsize=(12,8))
sns.barplot(x='City',y='R&D Expenditure (% of GDP)',color='lightblue',edgecolor='black',data=rd_expenditure_2024)
plt.title('R&D Expenditure (% of GDP) (2024)')
plt.xlabel('City')
plt.ylabel('R&D Expenditure (% of GDP)')
plt.xticks(rotation=45)
plt.show()

