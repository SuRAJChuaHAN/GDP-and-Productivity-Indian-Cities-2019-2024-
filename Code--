import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
import seaborn as sns

df = pd.read_excel('C:\\Users\\DELL\\Desktop\\GDP and Productivity of Indian Cities (2019-2024).xlsx')

dtypes=df.dtypes
print(dtypes)


# Description of Data
desc = df.describe()
print(desc)

plt.figure(figsize=(12,6))
desc.plot(kind='bar',edgecolor='black')
plt.title('Description of Data')
plt.xlabel('Column')
plt.ylabel('Values')
plt.show()


#Total of each city all over year R&D Expenditure 2019-2024
rd_expenditure = df.groupby('City')['R&D Expenditure (% of GDP)'].sum().reset_index()
print('R&D Expenditure (% of GDP) (2019-2024):\n',rd_expenditure)

# sns.heatmap(df.corr(),annot=True,cmap='YlGnBu')
# plt.show()

# df['City']=pd.to_numeric(df['City'])
# df['Year']=pd.to_numeric(df['Year'])

print(df.dtypes)
# Plot
plt.figure(figsize=(12,6))
plt.bar(rd_expenditure['City'],rd_expenditure['R&D Expenditure (% of GDP)'],color='skyblue',edgecolor='black')
plt.title(' R&D Expenditure (% of GDP) 2019-2024')
plt.xlabel('City')
plt.ylabel('R&D Expenditure (% of GDP)')
plt.xticks(rotation=45)
plt.show()

# Patenets per 100,000 Inhabitants 2019-2024
patents = df.groupby('City')['Patents per 100,000 Inhabitants'].sum().reset_index()
print('Patents per 100,000 Inhabitants (2019-2024):\n',patents)

# Plot
plt.figure(figsize=(12,6))
sns.barplot(x='City',y='Patents per 100,000 Inhabitants',data=patents,color='lightgreen',edgecolor='black')
plt.title('Patenets per 100,000 Inhabitants 2019-2024')
plt.xlabel('City')
plt.ylabel('Patents per 100,000 Inhabitants')
plt.xticks(rotation=45)
plt.show()

# Unemployment Rate (%) 2019 - 2024
unemployment_rate = df.groupby('City')['Unemployment Rate (%)'].sum().reset_index()
print('Unemployment Rate (%) (2019-2024):\n',unemployment_rate)

# Plot
plt.figure(figsize=(12,6))
sns.barplot(x='City', y='Unemployment Rate (%)', edgecolor='black',data=unemployment_rate)
plt.title('Unemployment Rate (%) 2019 - 2024')
plt.xlabel('City')
plt.ylabel('Unemployment Rate (%)')
plt.xticks(rotation=45)
plt.show()

# Youth Unemployment Rate (%) 2019-2024
youth_unemployed_rate = df.groupby('City')['Youth Unemployment Rate (%)'].sum().reset_index()
print('Youth Unemployment Rate (%) (2019-2024):\n',youth_unemployed_rate)

# Plot
plt.figure(figsize=(12,6))
sns.barplot(x='City',y='Youth Unemployment Rate (%)',edgecolor='black',data=youth_unemployed_rate)
plt.title('Youth Unemployment Rate (%) 2019-2024')
plt.xlabel('City')
plt.ylabel('Youth Unemployment Rate (%)')
plt.xticks(rotation=45)
plt.show()

#SME Employment (%) 2019-2024
sme_employment = df.groupby('City')['SME Employment (%)'].sum().reset_index()
print('SME Employment (%) (2019-2024):\n',sme_employment)

# Plot
plt.figure(figsize=(12,6))
sns.barplot(x='City',y='SME Employment (%)',edgecolor='black',data=sme_employment)
plt.title('SME Employment (%) 2019-2024')
plt.xlabel('City')
plt.ylabel('SME Employment (%)')
plt.xticks(rotation=45)
plt.show()

#Tourism Sector Employment (%) 2019-2024
tourism_sector_employment = df.groupby('City')['Tourism Sector Employment (%)'].sum().reset_index()
print('Tourism Sector Employment (%) (2019-2024):\n',tourism_sector_employment)

# Plot
plt.figure(figsize=(12,6))
plt.bar(tourism_sector_employment['City'],tourism_sector_employment['Tourism Sector Employment (%)'],edgecolor='black')
plt.title('Tourism Sector Employment (%) 2019-2024')
plt.xlabel('City')
plt.ylabel('Tourism Sector Employment (%)')
plt.xticks(rotation=45)
plt.show()

# ICT Sector Employment (%) 2019-2024
itc_sector_employment = df.groupby('City')['ICT Sector Employment (%)'].sum().reset_index()
print('ICT Sector Employment (%) (2019-2024):\n',itc_sector_employment)

# Plot
plt.figure(figsize=(12,6))
sns.barplot(x='City',y='ICT Sector Employment (%)',edgecolor='black',data=itc_sector_employment)
plt.title('ICT Sector Employment (%) 2019-2024')
plt.xlabel('City')
plt.ylabel('ICT Sector Employment (%)')
plt.xticks(rotation=45)
plt.show()

# Filter Rows each year (2019-2024)

year_2019 = df[df['Year']==2019]
year_2020 = df[df['Year']==2020]
year_2021 = df[df['Year']==2021]
year_2022 = df[df['Year']==2022]
year_2023 = df[df['Year']==2023]
year_2024 = df[df['Year']==2024]


# R&D Expenditure (% of GDP) (2019)
rd_expenditure_2019 = year_2019.groupby('City')['R&D Expenditure (% of GDP)'].sum().reset_index()
print('R&D Expenditure (% of GDP) (2019):\n',rd_expenditure_2019)


# Plot
plt.figure(figsize=(12,6))
plt.bar(rd_expenditure_2019['City'],rd_expenditure_2019['R&D Expenditure (% of GDP)'],color='lightgray',edgecolor='black')
plt.title('2019 R&D Expenditure (% of GDP) of All City')
plt.xlabel('City')
plt.ylabel('R&D Expenditure (% of GDP)')
plt.xticks(rotation=45)
plt.show()

# Patents per 100,000 Inhabitants (2019)
patents_2019 = year_2019.groupby('City')['Patents per 100,000 Inhabitants'].sum().reset_index()
print('Patents per 100,000 Inhabitants (2019)\n',patents_2019)

# Plot

plt.figure(figsize=(12,8))
sns.barplot(x='City',y='Patents per 100,000 Inhabitants',color='yellow',edgecolor='black',data=year_2019)
plt.title('Patents per 100,000 Inhabitants (2019)')
plt.xlabel('City')
plt.ylabel('Patents per 100,000 Inhabitants')
plt.xticks(rotation=45)
plt.show()

